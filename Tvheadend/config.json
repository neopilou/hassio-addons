{
  "name": "Tvheadend",
  "version": "0.03",
  "slug": "tvheadend",
  "description": "Tveheadend server",
  "url": "https://github.com/neopilou/hassio-addons",
  "startup": "services",
  "host_network": "true",
  "ingress": true,
  "ingress_port": 9981,
  "map": ["ssl"],
  "boot": "auto",
  "options": {
    "ssl": true,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem"
  },
  "ports": {
    "9981/tcp": null,
    "9982/tcp": 9982
  },
  "ports_description": {
    "9981/tcp": "For HTTP (web) server, Not required for Ingress",
    "9982/tcp": "For HTSP server"
  },
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "i386"
  ],
  "schema": {
    "certfile": "str",
    "keyfile": "str",
    "ssl": "bool"
  }  
}
