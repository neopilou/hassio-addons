{
  "name": "Tvheadend",
  "version": "0.0991",
  "slug": "tvheadend",
  "description": "Tveheadend server",
  "url": "https://github.com/neopilou/hassio-addons",
  "startup": "services",
  "webui": "http://[HOST]:[PORT:9981]",
  "host_network": "true",
  "boot": "auto",  
  "hassio_role": "default",
  "init": "false",
  "host_network": true,
  "host_pid": true,
  "ports": {
    "9981/tcp": 9981,
    "9982/tcp": 9982
  },
  "ports_description": {
    "9981/tcp": "For HTTP (web) server",
    "9982/tcp": "For HTSP server"
  },
  "apparmor": "false",
  "arch": [
    "aarch64",
    "amd64",
    "armhf"
  ] 
}
