{
  "name": "FTP Sync",
  "version": "0.054",
  "slug": "ftp_sync",
  "description": "Upload your Hass.io backups to FTP Server",
  "url": "https://github.com/neopilou/hassio-addons",
  "startup": "before",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "stdin": true,
  "hassio_api": true,
  "hassio_role": "manager",
  "boot": "auto",
  "map": ["backup"],
  "options": {
    "protocol": "ftp",
    "server": "server",
    "port": "21",
    "path": "homeassistant",
    "username": "username",
    "password": "password"
  },
  "schema": {
    "protocol": "str",
    "server": "str", 
    "port": "int",
    "path": "str",
    "username": "str",
    "password": "str",
    "keep_last": "int(0,)?"
  }
}
